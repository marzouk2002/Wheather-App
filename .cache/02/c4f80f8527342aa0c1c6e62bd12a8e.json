bold: 700;\n$fs-h1: 1.25rem;\n$fs-h2: 1.75rem;\n$fs-body: 1rem;\n$fs-xl: 4rem;\n$clr-light: #fff;\n$clr-gray: #989898;\n$clr-dark: #444;\n$clr-accent: #D06D6D;\n$clr-accent-dark: #613131;\n\n\n\n// Typographie\nh1, h2, h3 {\n    line-height: 1;\n}\n.main-title {\n    font-size: $fs-h1;\n    font-weight: $fw-light;\n    text-transform: uppercase;\n    letter-spacing: 2px;\n}\n\n.section-title {\n    font-size: $fs-h2;\n    font-weight: $fw-bold;\n    margin: 0;\n\n    &--date {\n        font-size: $fs-xl;\n    }\n}\n\na {\n    color: $clr-accent;\n\n    &:hover {\n        color: $clr-accent-dark;\n    }\n}\n","sass/main.scss":"@import 'config';\n\n.sr-only:not(:focus):not(:active) {\n    clip: rect(0 0 0 0);\n    clip-path: inset(50%);\n    height: 1px;\n    overflow: hidden;\n    position: absolute;\n    white-space: nowrap;\n    width: 1px;\n}\n\nbody {\n    margin: 0;\n    font-family: 'Montserrat', sans-serif;\n    line-height: 1.6;\n    background-image: linear-gradient(to right, rgb(255, 167, 35), rgb(255, 97, 35));\n    background-size:cover;\n    color: $clr-light;\n    min-height: 100vh;\n} \n\n.current {\n    background: rgba(0, 0, 0, .7);\n    margin: auto;\n    padding: 2rem;\n    max-width: 1000px;\n    \n    &_input {\n        grid-column: 1/-1;\n        & > input {\n            width: 80%;\n            background-color: white;\n            border: none;\n        }\n    }\n\n    &_city {\n        grid-column: 1/4;\n        grid-row: 2/3;\n\n        &__desc {\n            font-size: $fs-h2;\n            color: $clr-gray;\n            font-weight: $fw-light;\n            margin: 0;\n        }\n    }\n\n    &_sunrise {\n        grid-area: 3/1/4/2;\n        padding: 0 2em;\n    }\n    &_sunset {\n        grid-area: 4/1/5/2;\n        padding: 0 2em;\n    }\n\n    &_temp,\n    &_presip {\n        grid-column: 2/3;\n        padding: 0 2em;\n        @include border-right-left;\n    }\n\n    &_wind {\n        grid-column: 3/4;\n        grid-row: 3/4;\n\n        display: grid;\n        grid-template-columns: repeat(2, 1fr);\n        grid-template-rows: min-content, 1fr;\n        align-self: start;\n\n        .wind {\n            &__direction {\n                $size : 6rem;\n                width: $size;\n                height: $size;\n                border-radius: 50%;\n                background-color: rgba(255, 255, 255, 0.5);\n                display: grid;\n                place-items: center;\n                grid-row: 1/ span 2;\n            }\n\n            &__arrow {\n                --direction:0deg;\n                $size: 1rem;\n                width: $size;\n                height: $size*3;\n                background-color: $clr-accent-dark;\n                clip-path: polygon(50% 0%, 0% 100%, 100% 100%);\n                transform-origin: bottom center;\n                transform: translateY(-50%)  rotate(var(--direction));\n            }\n        }\n    }\n\n    &_unit {\n        grid-column: 3/4;\n        place-self: end;\n        color: $clr-light;\n        display: flex;\n        align-items: center;\n        opacity: .6;\n        transition: opacity 300ms linear;\n\n        &:hover {\n            opacity: 1;\n        }\n        \n        label {\n            cursor: pointer;\n        }\n\n        input {\n            clip: rect(0 0 0 0);\n            clip-path: inset(50%);\n            height: 13px;\n            overflow: hidden;\n            position: absolute;\n            white-space: nowrap;\n            width: 13px;\n        }\n\n        .unit__toggle {\n            cursor: pointer;\n            border: 2px solid $clr-light;\n            background-color: transparent;\n            width: 4em;\n            border-radius: 100vmax;\n            padding: 0;\n            margin: 0 .5rem;\n            outline: none;\n\n            &::after {\n                content: '';\n                display: block;\n                height: 1rem;\n                margin: 2px;\n                margin-left: auto;\n                width: 1rem;\n                background-color: $clr-light;\n                border-radius: 50%;\n            }\n        }\n\n        :checked ~ .unit__toggle::after {\n            margin-left: 2px;\n        }\n    }\n}\n\n.reading {\n    font-size: $fs-h1;\n    color: $clr-gray;\n    margin: 0;\n}\n\n@media (min-width: 900px ) {\n\n    body {\n        height: 100vh;\n        overflow: hidden;\n    }\n\n    .current {\n        margin: 4rem 0 0 4rem;\n        display: grid;\n        grid-template-columns: repeat(3, 1fr);\n        grid-gap: 2rem;\n    }\n}\n\n@media (max-width: 900px) {\n    .current {\n        display: grid;\n        grid-template-columns: 100%;\n        grid-template-rows: repeat(7, 1fr);\n\n        &_input {\n            display: flex;\n            align-items: center;\n        }\n\n        &_sunrise {\n            grid-row: 3/4;\n            @include border-right-left;\n        }\n\n        &_sunset {\n            grid-row: 4/5;\n            grid-column: 1/2;\n            @include border-right-left;\n        }\n\n        &_temp {\n            grid-row: 5/6;\n            grid-column: 1/2;\n        }\n\n        &_presip {\n            grid-row: 6/7;\n            grid-column: 1/2;\n        }\n\n        &_wind {\n            grid-row: 7/8;\n            grid-column: 1/2;\n            @include border-right-left;\n\n            h2,p {\n                padding-left: 2rem;\n            }\n            .wind__direction {\n                grid-row: 1/ span 3;\n                grid-column: 2/3;\n            }\n        }\n\n        &_unit {\n            grid-row: 8/9;\n            grid-column: 1/2;\n            place-self: center;\n            justify-content: center;\n            padding-top: 1rem;\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"2fb5532af780b1d8cb732ef21596ef40","cacheData":{"env":{}}}